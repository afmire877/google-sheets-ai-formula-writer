{
  "timeZone": "America/New_York",
  "dependencies": {
    "enabledAdvancedServices": [
      {
        "userSymbol": "Sheets",
        "serviceId": "sheets",
        "version": "v4"
      }
    ]
  },
  "exceptionLogging": "STACKDRIVER",
  "runtimeVersion": "V8",
  "addOns": {
    "common": {
      "name": "AI Formula Writer",
      "logoUrl": "https://developers.google.com/workspace/add-ons/images/calculator-logo.png",
      "useLocaleFromApp": true
    },
    "sheets": {
      "homepageTrigger": {
        "runFunction": "onHomepage"
      },
      "onSelectionChange": {
        "runFunction": "onSelectionChange"
      }
    }
  },
  "webapp": {
    "access": "ANYONE",
    "executeAs": "USER_DEPLOYING"
  },
  "oauthScopes": [
    "https://www.googleapis.com/auth/spreadsheets.currentonly",
    "https://www.googleapis.com/auth/script.external_request"
  ]
}